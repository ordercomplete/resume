<div>
  <section>
    <h1>{{name}}</h1>
    <h2><q>{{description}}</q></h2>
    <section>
      <h3>History:</h3>
      <section>
        <h4>{{#with history}}
          <p>year - {{year}}</p>
          <p>founder- {{founder}}</p>
          {{/with}}
        </h4>
      </section>
    </section>

    <h4>Types:</h4>
    <h5>{{#each types}}
      <li>{{this}}</li>
      {{/each}}
    </h5>

    <section>
      <h5>Syntax:</h5>
      <dl>
        {{#with syntax}}
        <ol>
          <li>
            <b>variables:</b>
            <ul>
              {{#each variables}}
              <li>{{this}}</li>
              {{/each}}
            </ul>
          </li>

          <li>
            <b>functions:</b>
            <ul>
              {{#each functions}}
              <li>{{this}}</li>
              {{/each}}
            </ul>
          </li>

          <li>
            <b>conditionals:</b>
            <ul>
              {{#each conditionals}}
              <li>{{this}}</li>
              {{/each}}
            </ul>
          </li>

          <li>
            <b>loops:</b>
            <ul>
              {{#each loops}}
              <li>{{this}}</li>
              {{/each}}
            </ul>
          </li>

          <li>
            <b>classes:</b>
            <ul>
              {{#each classes}}
              <li>{{this}}</li>
              {{/each}}
            </ul>
          </li>
        </ol>
        {{/with}}
    </section>

    <section>
      <h5>Features:</h5>
      <ul>
        {{#each features}}
        <li>{{this}}</li>
        {{/each}}
      </ul>
    </section>

    <section>
      <h5>Libraries:</h5>
      <ul>
        {{#each libraries}}
        <li>{{this}}</li>
        {{/each}}
      </ul>
    </section>

    <section>
      <h5>Tools:</h5>
      <ul>
        {{#each tools}}
        <li>{{this}}</li>
        {{/each}}
      </ul>
    </section>

    <section>
      <h5>Community:</h5>
      <ul>
        {{#each community}}
        <li>{{name}} ({{type}})</li>
        {{/each}}
      </ul>
    </section>
  </section>
</div>